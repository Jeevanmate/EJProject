<div class="alert alert-secondary mt-4">
    <span class="oi oi-pencil me-2" aria-hidden="true"></span>
    <strong>@Title</strong>

    <span class="text-nowrap">
        Please take our
        <a target="_blank" class="font-weight-bold link-dark" href="https://docs.google.com/forms/d/e/1FAIpQLSe3KoiraM2ydYyW8iS_yFtfRaC1DeEzwFC6JgISGl3xOvBw1w/viewform?usp=pp_url&entry.1510756417=Yes">brief survey</a>
    </span>
    and tell us what you think.
</div>

@code {
    // Demonstrates how a parent component can supply parameters
    [Parameter]
    public string? Title { get; set; }
}
